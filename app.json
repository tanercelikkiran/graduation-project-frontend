{
  "expo": {
    "name": "Edifica",
    "slug": "edifica",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "edifica",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.edifica",
      "versionCode": 1,
      "navigationBar": {
        "visible": "immersive-sticky"
      },
      "statusBar": {
        "hidden": false,
        "style": "auto"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "image": "./assets/images/icon.png",
            "backgroundColor": "#131313"
          }
        }
      ],
      "expo-font",
      "expo-asset",
      "expo-navigation-bar"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "fb96d606-3883-4328-b032-e5b656567fd3"
      }
    },
    "owner": "tanercelikkiran"
  }
}